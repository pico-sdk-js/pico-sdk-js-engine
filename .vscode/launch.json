{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch local",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/pico-sdk-js-0.0.1",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch remote",
            "type": "gdb",
            "request": "attach",
            "gdbpath": "/usr/bin/gdb-multiarch",
            "executable": "${workspaceFolder}/build/pico-sdk-js-0.0.1.elf",
            "cwd": "${workspaceFolder}",
            "target": "localhost:3333",
            "remote": true,
            "printCalls": true,
            "valuesFormatting": "parseText"
        }

    ]
}